{
  "extends": "airbnb-base",
  "rules": {
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": true,
        "variables": true
      }
    ],
    "no-param-reassign": [
      "error",
      { 
        "props": false 
      }
    ],
    "object-shorthand": "off",
    "no-trailing-spaces": "off",
    "comma-dangle": "off"
  }
}
